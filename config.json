    "title":"GhostDavid的博客",    //【必填】博客标题
    "subTitle":"Github永存，联系博主：marl_table67@icloud.com",    //【必填】博客描述&自述
    "avatarUrl":"https://github.githubassets.com/favicons/favicon.svg",    //【必填】博客头像
    "GMEEK_VERSION":"last",
    "startSite":"08/09/2024",    //用于页面底部显示网站运行天数
    "onePageListNum":20,    //用于首页每页展示的文章数量
