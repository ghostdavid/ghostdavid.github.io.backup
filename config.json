    "themeMode":"fix",    //用于固定定义亮主题
    "dayTheme":"light_tritanopia",
    "nightTheme":"github-light",
    "urlMode":"pinyin",    //用于定义文章链接生成模式，目前支持pinyin/issue/ru_translit
