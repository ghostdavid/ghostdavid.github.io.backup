    "rssSplit":"sentence",    //设置RSS输出的截断符号，默认sentence为第一句话，可配置其他特殊符号
    "bottomText":"转载和引用须注明出处-<a href='https://creativecommons.org/licenses/by-nc-sa/4.0/deed.zh-hans'>CC BY-NC-SA 4.0协议</a>",    //用于设置文章页面右下角显示的内容
    "needComment":1,    //用于设置是否需要评论功能，1开启评论，0关闭
    "allHead":"<script src='https://ghostdavid.github.io/Vercount.js'></script>",    //用于自定义所有页面head内容
